    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-native": "0.51.0",
    "react-navigation": "^1.0.0-beta.21",
    "react-redux": "^5.0.6"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "21.2.0",
    "babel-plugin-dva-hmr": "^0.3.2",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
